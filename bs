local a=game.Players.LocalPlayer
local b=loadstring(game:HttpGet("https://raw.githubusercontent.com/0-BaconScripter-0/Mobile-Support-For-UI-Library/refs/heads/main/Unnamed.lua"))()
local c=b:CreateWindow("7 days to live")
local d=c:AddFolder("toggles")
local e=c:AddFolder("buttons")

_G.r=75

d:AddSlider({text="kill aura range",min=1,max=5000,float=1,callback=function(v)_G.r=v end})

d:AddToggle({text="aura kill",state=false,callback=function(v)_G.ak=v if v then task.spawn(function()while _G.ak do local f=a.Character local g=f and f:FindFirstChild("HumanoidRootPart") if g then for _,h in ipairs(workspace.scps:GetChildren())do local i=h:FindFirstChild("HumanoidRootPart")or h:FindFirstChild("Head") local j=h:FindFirstChildWhichIsA("Humanoid") if i and j and j.Health>0 then if (i.Position-g.Position).Magnitude<_G.r then game.ReplicatedStorage.remotes.shoot:FireServer(i.CFrame+Vector3.new(0,0.5,0),i.CFrame) end end end for _,h in ipairs(workspace.animals:GetChildren())do local i=h:FindFirstChild("HumanoidRootPart")or h:FindFirstChild("Head") local j=h:FindFirstChildWhichIsA("Humanoid") if i and j and j.Health>0 then if (i.Position-g.Position).Magnitude<_G.r then game.ReplicatedStorage.remotes.shoot:FireServer(i.CFrame+Vector3.new(0,0.5,0),i.CFrame) end end end if workspace:FindFirstChild("enemies")then for _,h in ipairs(workspace.enemies:GetChildren())do local i=h:FindFirstChild("Head") local j=h:FindFirstChildWhichIsA("Humanoid") if i and j and j.Health>0 then if (i.Position-g.Position).Magnitude<_G.r then game.ReplicatedStorage.remotes.shoot:FireServer(i.CFrame+Vector3.new(0,0.5,0),i.CFrame) end end end end end task.wait(0.25)end end)end end})

d:AddToggle({text="esp spy",state=false,callback=function(v)_G.esp=v if not _G.esps then _G.esps={}end local function k()for _,h in pairs(_G.esps)do if h and h.Parent then h:Destroy()end end _G.esps={}end if not v then k()return end task.spawn(function()while _G.esp do for l,h in pairs(_G.esps)do if not l.Parent then h:Destroy()_G.esps[l]=nil end end for _,m in ipairs(workspace:FindFirstChild("enemies")and workspace.enemies:GetChildren()or{})do if m:FindFirstChild("Head")and not _G.esps[m]then local n=Instance.new("BillboardGui")n.Adornee=m.Head n.Size=UDim2.new(0,200,0,50)n.StudsOffset=Vector3.new(0,2,0)n.AlwaysOnTop=true local o=Instance.new("TextLabel")o.Size=UDim2.new(1,0,1,0)o.BackgroundTransparency=1 o.Text="SPY"o.TextColor3=Color3.fromRGB(255,0,0)o.TextStrokeTransparency=0 o.TextScaled=true o.Font=Enum.Font.GothamBold o.Parent=n n.Parent=m.Head _G.esps[m]=n end end task.wait(0.2)end k()end)end})

d:AddToggle({text="auto farm cordage",state=false,callback=function(v)_G.af=v if v then task.spawn(function()local g=a.Character:WaitForChild("HumanoidRootPart")while _G.af do for _,h in pairs(workspace.harvest:GetChildren())do if not _G.af then break end if h.Name:lower()=="flax"then local i=h:FindFirstChildWhichIsA("BasePart")if i then g.CFrame=i.CFrame+Vector3.new(0,2,0)task.wait(0.1)for _,j in pairs(h:GetDescendants())do if j:IsA("ProximityPrompt")then fireproximityprompt(j)end end end end end task.wait()end end)end end})

d:AddToggle({text="active proximity prompts",state=false,callback=function(v)_G.ap=v if v then task.spawn(function()local g=a.Character:WaitForChild("HumanoidRootPart")while _G.ap do for _,h in pairs(workspace:GetDescendants())do if h:IsA("ProximityPrompt")and h.Parent:IsA("BasePart")and(g.Position-h.Parent.Position).Magnitude<15 then fireproximityprompt(h)end end task.wait(0.05)end end)end end})

d:AddToggle({text="auto fish",state=false,callback=function(v)_G.afs=v if v then task.spawn(function()task.spawn(function()while _G.afs and task.wait(0.01)do game.ReplicatedStorage.remotes.fish_point:FireServer(true)end end)task.spawn(function()local p=game:GetService("ReplicatedStorage"):WaitForChild("remotes")local q=p:WaitForChild("cast")while _G.afs do local r=a.Character or a.CharacterAdded:Wait()local g=r:WaitForChild("HumanoidRootPart")local s=g.CFrame.LookVector local t=g.Position+(s*50)pcall(function()q:FireServer(t)end)task.wait(0.5)end end)end)end end})

e:AddButton({text="teleport to spawn",callback=function()local u=workspace:FindFirstChild("SpawnLocation")if u and a.Character and a.Character:FindFirstChild("HumanoidRootPart")then a.Character.HumanoidRootPart.CFrame=u.CFrame end end})
e:AddButton({text="fly gui",callback=function()loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Fly-v3-13879"))()end})
e:AddButton({text="teleport to heli crash",callback=function()local g=a.Character and a.Character:FindFirstChild("HumanoidRootPart")if not g then return end for _,h in pairs(workspace:WaitForChild("interact"):GetChildren())do if h.Name=="heli_crash"and h:FindFirstChild("Body")then local i=h.Body:FindFirstChildWhichIsA("BasePart")if i then g.CFrame=i.CFrame+Vector3.new(0,3,0)break end end end end})
e:AddLabel({text="Made by Pall(@opalbetus)"})
e:AddLabel({text="https://discord.gg/TbK2NWqS5R"})
b:Init()